---
import Upload from '@assets/button/upload.svg';
import { Image } from 'astro:assets';
---

<Image src={Upload} alt="Upload" decoding="async" loading="lazy" class="cursor-pointer" id="UploadModal" />

<script>
  const uploadModel = document.querySelector('#UploadModal');
  uploadModel?.addEventListener('click', () => {
    window.location.href = '/official/upload';
  });
</script>
